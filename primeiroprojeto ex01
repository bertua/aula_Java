import java.util.Scanner;
public class Ex01 {
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        int soma = 0;
        double media;
        int numero = input.nextInt();
        int i = 0;
        while(numero >= 0){
            soma += numero;
            numero = input.nextInt();
            i++;
        }
        media = soma/i;
        System.out.println(media);
    }
    
}
